{
  "name": "almighty-tool",
  "version": "0.0.18",
  "description": "Almighty Tool",
  "author": "happy",
  "private": false,
  "license": "MIT",
  "_moduleAliases": {
    "@root": "."
  },
  "scripts": {
    "eslint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "eslint:fix": "yarn eslint --fix",
    "stylelint": "stylelint \"{src,source}/**/*.{css,scss,sass}\" --allow-empty-input",
    "stylelint:fix": "yarn stylelint --fix",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:fix": "yarn run eslint:fix && yarn run stylelint:fix",
    "tsc": "./node_modules/.bin/tsc -p tsconfig.json",
    "check": "npm run test && npm run eslint",
    "test": "jest --clearCache && jest --runInBand --detectOpenHandles"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  },
  "typings": "types/index.d.ts",
  "dependencies": {
    "base64-js": "^1.5.1",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.3.0",
    "esno": "^4.8.0",
    "is-what": "^4.1.8",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "nock": "^13.3.0",
    "node-forge": "^1.3.1",
    "node-rsa": "^1.1.1",
    "qs": "^6.11.0",
    "tslib": "^2.4.0",
    "typescript": "~4.5.5",
    "uuid": "^9.0.0",
    "yargs": "^17.7.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/debug": "^4.1.8",
    "@types/html5plus": "1.0.2",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.14.2",
    "@types/node-forge": "^1.3.11",
    "@types/node-rsa": "^1.1.4",
    "@types/power-assert": "1.5.8",
    "@types/qs": "6.9.7",
    "@types/uuid": "9.0.1",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.4.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-nuxt": ">=3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "9.1.1",
    "eslint-plugin-vue-scoped-css": "2.2.0",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "mockdate": "^3.0.5",
    "power-assert": "^1.6.1",
